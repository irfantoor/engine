{
    "name": "irfantoor/engine",
    "description": "A bare-minimum PHP framework, focussing on the requests and the responses. A swiss-knife for world-wide-web",
    "keywords": ["irfans-engine", "mini", "php", "framework", "psr-7"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Irfan TOOR",
            "email": "email@irfantoor.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "irfantoor/debug": "^0.2.1",
        "irfantoor/container": "^0.9.1",
        "psr/http-message": "^1.0",
        "fig/http-message-util": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4"
    },
    "provide": {
        "psr/http-message": "1.0.1",
        "fig/http-message-util": "1.1.2"
    },
     "bin": [
        "ie"
    ],
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "IrfanTOOR\\Engine\\": "src"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"symlink('vendor/bin/ie', 'ie');\""
        ]
    }
}
