{
    "name": "irfantoor/engine",
    "description": "A bare-minimum PHP framework, focussing on the requests and the responses. A swiss-knife for world-wide-web",
    "keywords": ["irfans-engine", "mini", "php", "framework", "psr-7"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Irfan TOOR",
            "email": "email@irfantoor.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "irfantoor/debug": "^0.2.1",
        "irfantoor/container": "^0.9.1",
        "irfantoor/database": "^0.1.2",
        "league/flysystem": "^1.0",
        "latte/latte": "^2.4",
        "psr/http-message": "^1.0",
        "fig/http-message-util": "^1.1",
        "cebe/markdown": "^1.1",
        "tracy/tracy": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "provide": {
        "psr/http-message": "1.0.1",
        "fig/http-message-util": "1.1.2"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "App\\": "app",
            "IrfanTOOR\\Engine\\": "src"
        }
    },
    "scripts": {
        "post-update-cmd": "IrfanTOOR\\Engine\\Command\\App::copyIe",
        "post-install-cmd": [],
        "post-autoload-dump": [],
        "post-create-project-cmd": []
    }
}
