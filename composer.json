{
    "name": "irfantoor/engine",
    "description": "A bare-minimum PHP framework, focussing on the requests and the responses. A swiss-knife for world-wide-web",
    "keywords": ["irfans-engine", "mini", "php", "framework"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Irfan TOOR",
            "email": "email@irfantoor.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "irfantoor/collection": "^1.0",
        "irfantoor/container": "^0.9.0",
        "irfantoor/console": "^0.2.0",
        "fig/http-message-util": "^1.1",
        "guzzlehttp/streams": "^3.0",
        "league/flysystem": "^1.0",
        "monolog/monolog": "^1.23",
        "erusev/parsedown": "^1.6",
        "middlewares/utils": "^0.14.0",
        "http-interop/response-sender": "^1.0",
        "http-interop/http-factory-tests": "^0.3.0",
        "http-interop/http-server-handler": "^1.0",
        "http-interop/http-server-middleware": "^1.0",
        "http-interop/http-factory": "^0.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "IrfanTOOR\\Engine\\": "src"
        }
    }
}
